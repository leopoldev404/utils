{
  "editor.insertSpaces": false,
  "editor.scrollBeyondLastLine": false,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.scrollbar.horizontalScrollbarSize": 8,
  "window.title": "${dirty}${activeEditorShort}",
  "workbench.statusBar.visible": false,
  "workbench.tree.renderIndentGuides": "none",
  "remote.autoForwardPorts": false,
  "explorer.compactFolders": false,
  "zenMode.hideActivityBar": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.fontFamily": "jetbrains mono",
  "editor.lineHeight": 1.7,
  "editor.fontSize": 13,
  "editor.inlayHints.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "editor.fontLigatures": true,
  "editor.codeLensFontFamily": "Jetbrains Mono",
  "editor.lineNumbers": "off",
  "markdown.preview.fontFamily": "Segoe UI",
  "editor.cursorStyle": "underline",
  "editor.cursorBlinking": "smooth",
  "workbench.startupEditor": "none",
  "workbench.settings.editor": "json",
  "terminal.integrated.fontFamily": "Jetbrains Mono",
  "terminal.integrated.defaultProfile.windows": "Dev",
  "terminal.integrated.cursorStyle": "underline",
  "editor.renderWhitespace": "none",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Dev": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "overrideName": true,
      "icon": "terminal-bash",
      "color": "terminal.ansiCyan"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.cursorBlinking": true,
  "editor.roundedSelection": true,
  "editor.guides.indentation": false,
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.git": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/obj": true,
    "**/bin": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*.Identifier": true
  },
  "explorer.confirmDragAndDrop": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "window.titleBarStyle": "custom",
  "terminal.integrated.fontSize": 13,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "[One Dark Pro]": {
      "editor.background": "#282C34",
      "activityBar.background": "#282C34",
      "sideBar.background": "#282C34",
      "titleBar.activeBackground": "#282C34",
      "statusBar.background": "#282C34",
      "terminal.background": "#282C34",
      "terminal.foreground": "#ABB2BF",
      "tab.activeBorder": "#282C34",
      "tab.inactiveBackground": "#282C34",
      "panel.background": "#282C34",
      "statusBar.border": "#282C34",
      "titleBar.border": "#282C34",
      "editor.lineHighlightBackground": "#282C34"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Default Dark Modern]": {
      "keywords": "#569CD6"
    },
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter", "variable.other.readwrite"],
          "settings": {
            "foreground": "#ABB2BF"
          }
        }
      ]
    }
  },
  "redhat.telemetry.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
